
fonction "tableau.plus" (x) fait 
	!tableau_plus(self, 1);
	self.taille = 1;
	!tableau_plus(self, 2);
	self.taille = 2;
	!tableau_plus(self, 3);
	self.taille = 3;
	!tableau_plus(self, 4);
