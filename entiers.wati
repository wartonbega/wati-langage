fonction <ent> "ent._puissance" (<ent> soit, <ent> exp) fait  
    res = 1;
    tant que exp > 0 fait 
        res = res * soit;
        exp = exp - 1;
    fin
    renvoie res;
fin

fonction <ent> puissance_dix(<ent> x) fait 
    <ent> l = 1;
    <ent> c = 0;
    tant que x / l > 0 fait 
        l = l * 10;
        c = c + 1;
    fin
    renvoie c;
fin

