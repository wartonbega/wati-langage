!mainloop Démmare la boucle principale d'execution. !mainloop( fonction_execution(fonction) ) -> 0
!init_window Initialise la fenêtre SDL. !init_window( largeur(int), longueur(int) ) -> 0
!quit_window Ferme et quitte SLD (et donc la fenêtre crée). !quit_window( ) -> 0
!fill_rect Remplis le contenue d'un rectange de la couleur actuelle. !fill_rect( x(int), y(int), longueur(int), largeur(int) ) -> 0
!draw_line Trace une ligne entre les points indiqués. !fill_rect( x(int), y(int), x_prime(int), y_prime(int) ) -> 0
!set_color Met la couleur indiquée (rvba) comme couleur actuelle. !set_color( rouge(int), vert(int), bleu(int), alpha(int) ) -> 0
!get_mouse_pos Renvoie les coordonnées actuelles de la souris. !get_mouse_pos( ) -> list[ x(int), y(int) ]
