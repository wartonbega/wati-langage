inclue "./lib/std.wati";

fonction loop (event) fait
    si event.taille > 0 alors
        si (event[0] == "Escape") alors
            renvoie 0;
        fin
    fin
    !clear()
    !draw_texture("wati", 0, 0, 30, 30)
    !render()
    renvoie 1;
fin

!init_window(480, 480)
!load_texture_as("wati.png", "wati")
!mainloop(!loop)